  "scripts": {
    "start": "wireit",
    "build": "wireit",
    "tsc": "wireit",
    "tsc:watch": "wireit",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "wireit": {
    "start": {
      "env": {
        "NODE_OPTIONS": "--experimental-vm-modules"
      },
      "command": "concurrently \"npm run tsc:watch\" \"eleventy --serve\"",
      "dependencies": [
        "tsc"
      ]
    },
    "build": {
      "env": {
        "NODE_OPTIONS": "--experimental-vm-modules"
      },
      "command": "eleventy",
      "dependencies": [
        "tsc"
      ]
    },
    "tsc": {
      "command": "tsc"
    },
    "tsc:watch": {
      "command": "tsc --watch"
    }
  },
  "keywords": [],
  "author": "",
