    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@web/test-runner": "^0.15.1",
    "concurrently": "^7.6.0",
    "esbuild": "^0.17.11",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-lit": "^1.8.2",
    "glob": "^9.2.1",
